    <owl:Class rdf:about="http://purl.obolibrary.org/obo/STATO_0000122">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000038"/>
        <obo:IAO_0000115 xml:lang="en">a pedigree chart is a graph which plots parent child relations</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/stato.owl"/>
        <rdfs:label xml:lang="en">pedigree chart</rdfs:label>
