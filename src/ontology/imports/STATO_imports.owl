        <obo:IAO_0000115 xml:lang="en">a real time quantitative pcr plot is a line graph which plots the signal fluorescence intensity as a function of the number of PCR cycle</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/stato.owl"/>
        <rdfs:label xml:lang="en">real time quantitative pcr plot</rdfs:label>
    </owl:Class>
    


